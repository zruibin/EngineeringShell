[
    {
        "action": "git",
        "fileName": "ninja",
        "url": "git clone -b v1.11.1 --depth=1 https://github.com/ninja-build/ninja.git",
        "build": "cmake",
        "args": ""
    }

    ,
    {
        "action": "git",
        "fileName": "json",
        "url": "git clone -b v3.11.3 --depth=1 https://github.com/nlohmann/json.git",
        "build": "cmake",
        "args": "-DJSON_BuildTests=OFF"
    }

    ,
    {
        "action": "git",
        "fileName": "websocketpp",
        "url": "git clone -b 0.8.2 --depth=1 https://github.com/zaphoyd/websocketpp.git",
        "build": "cmake",
        "args": "-DBUILD_EXAMPLES=0 -DBUILD_TESTS=0"
    }

    ,
    {
        "action": "git",
        "fileName": "hiredis",
        "url": "git clone -b v1.2.0 --depth=1 https://github.com/redis/hiredis.git",
        "build": "cmake",
        "args": [
            "-DBUILD_SHARED_LIBS=OFF", 
            "-DDISABLE_TESTS=ON",
            "-DENABLE_NUGET=OFF"
        ]
    }

    ,
    {
        "action": "git",
        "fileName": "redis-plus-plus",
        "url": "git clone -b 1.3.14 --depth=1 https://github.com/sewenew/redis-plus-plus.git",
        "build": "cmake",
        "args": [
            "-DREDIS_PLUS_PLUS_BUILD_TEST=OFF",
            "-DREDIS_PLUS_PLUS_BUILD_SHARED=OFF",
            "-DREDIS_PLUS_PLUS_BUILD_STATIC_WITH_PIC=OFF"
        ]
    }

