{
  "name": "engineering-shell-tauri",
  "productName": "Engineering Shell",
  "description": "A Engineering Shell",
  "author": "Ruibin.Chow",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint --config eslint.config.mjs --cache-location=.cache/ --cache .",
    "lint:fix": "eslint --config eslint.config.mjs --cache-location=.cache/ --cache --fix .",
    "delete": "node --require esbuild-register ./script/delete.ts",
    "delete:all": "node --require esbuild-register ./script/delete.ts --all",
    "move": "node --require esbuild-register ./script/move.ts",
    "start:rspack": "cross-env NODE_ENV=development rspack serve --config=webpack.config.js",
    "build:rspack": "cross-env NODE_ENV=production rspack --config webpack.config.js",
    "start:webpack": "cross-env NODE_ENV=development webpack serve --config=webpack.config.js",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "prestart": "npm run lint:fix",
    "start": "npm run start:rspack",
    "build": "npm run build:rspack",
    "dev": "npx tauri dev",
    "preprod": "npm run delete:all",
    "prod": "npx tauri build",
    "postprod": "npm run move",
    "dev:ios": "npx tauri ios dev",
    "prod:ios": "npx tauri ios build",
    "dev:android": "npx tauri android dev",
    "prod:android": "npx tauri android build"
  },
  "license": "MIT",
  "dependencies": {
    "@tauri-apps/api": "^2.3.0",
    "antd": "^5.24.1",
    "fs-extra": "^11.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@eslint/js": "^9.21.0",
    "@rspack/cli": "^1.2.8",
    "@rspack/core": "^1.2.8",
    "@tauri-apps/cli": "^2.3.1",
    "@types/node": "^22.13.5",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "babel-loader": "^9.2.1",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "esbuild": "^0.25.0",
    "esbuild-register": "^3.6.0",
    "eslint": "^9.21.0",
    "eslint-import-resolver-typescript": "^3.8.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "espree": "^10.3.0",
    "globals": "^16.0.0",
    "html-webpack-plugin": "^5.6.3",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.7.3",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0",
    "webpack-node-externals": "^3.0.0",
    "webpack-shell-plugin-next": "^2.3.2"
  }
}
